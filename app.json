
{
  "expo": {
    "name": "AI Workout Builder",
    "slug": "aiworkoutbuilder-app",
    "scheme": "aiworkoutbuilder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/app-icon-mlj.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/final_quest_240x240.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.aiworkoutbuilder.app",
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera to allow you to take progress photos.",
        "NSPhotoLibraryUsageDescription": "This app uses the photo library to allow you to select progress photos."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/final_quest_240x240.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.aiworkoutbuilder.app"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/final_quest_240x240.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "YOUR_EAS_PROJECT_ID"
      }
    }
  }
}
